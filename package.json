{
  "name": "redux-filterlist",
  "version": "0.1.1",
  "description": "A Higher Order Component using react-redux for creating lists with filters, sotring, paginatinon, endless scroll etc.",
  "main": "./dist/redux-filterlist.js",
  "scripts": {
    "build": "npm run build:lib && npm run build:umd && npm run build:umd:min",
    "build:lib": "babel src --out-dir lib --ignore __tests__,test-utils",
    "build:umd": "cross-env NODE_ENV=development webpack",
    "build:umd:min": "cross-env NODE_ENV=production webpack",
    "lint": "eslint src",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vtaits/redux-filterlist.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "redux-filterlist",
    "filterlist",
    "filter",
    "sort",
    "pagination",
    "table"
  ],
  "author": "Vadim Taits",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vtaits/redux-filterlist/issues"
  },
  "homepage": "https://github.com/vtaits/redux-filterlist#readme",
  "peerDependencies": {
    "react": "^15.0.0",
    "react-redux": "^4.3.0 || ^5.0.0",
    "redux": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-plugin-jest": "^20.0.1",
    "eslint-plugin-react": "^7.0.0",
    "flux-standard-action": "^1.2.0",
    "jest": "^20.0.4",
    "path": "^0.12.7",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.1",
    "sinon": "^2.3.6",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  }
}
